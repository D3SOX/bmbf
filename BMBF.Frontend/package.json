{
  "name": "bmbf-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently -k -r 'npm:*:dev'",
    "backend:dev": "cd ../BMBF.Desktop && dotnet run",
    "vite:dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" --ignore-path .gitignore",
    "lint": "eslint ./src --ext .jsx,.js,.ts,.tsx --ignore-path .gitignore"
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@mantine/core": "^5.2.0",
    "@mantine/hooks": "^5.2.0",
    "@mantine/notifications": "^5.2.0",
    "@tabler/icons": "^1.83.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "valtio": "^1.6.3"
  },
  "devDependencies": {
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "@vitejs/plugin-react": "^2.0.1",
    "concurrently": "^7.3.0",
    "eslint": "^8.22.0",
    "eslint-plugin-valtio": "^0.6.0",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.8"
  }
}
